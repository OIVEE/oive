<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="resource.PublicationsMapper">
   	<select id="listAll" parameterType="string" resultType="vo.PublicationsVO">
    	select ID,USERID,SKILLSNAME,SKILLSLEVEL,SKIILSCOMMENTS
    	where userId = #{userId}    	
 	</select>
 	
 	<select id="seqid" resultType="int">
 	select publication_SEQ.nextval from dual 	
 	</select>
 	<insert id="insert" parameterType="vo.PublicationsVO"> 		
		insert into Publications(ID,USERID,SKILLSNAME,SKILLSLEVEL,SKIILSCOMMENTS
		values(#{id}, #{userId}, #{skillsName}, #{level}, #{skillsComments})
 	</insert>

 	<update id="edit" parameterType="vo.PublicationsVO">
 		update Publications set 			
			skillsName = #{skillsName},
			level = #{level},
			skillsComments = #{skillsComments}
		where USERID = #{userId} and id = #{id}
 	</update>
</mapper>








